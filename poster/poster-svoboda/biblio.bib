@article{ ISI:000083492100006,
Author = {Krlin, L and Klima, R and Pavlo, P and Petrzilka, V and Svoboda, V and
   Tataronis, JA},
Title = {{Transport of intense LH pulses into a tokamak plasma}},
Journal = {{JOURNAL OF PLASMA PHYSICS}},
Year = {{1999}},
Volume = {{62}},
Number = {{Part 2}},
Pages = {{203-218}},
Month = {{AUG}},
Abstract = {{Application of lower-hybrid (LH) power in short, intense pulses in the
   5-10 GW range should overcome the limiting effects of Landau damping,
   and thereby permit the penetration of LH power into the interior of
   large-scale plasmas. We show that, under such very intense LH pulses,
   wave coupling may deteriorate because of nonlinear density changes due
   to the ponderomotive force Effects in front of the grill. Ponderomotive
   for ces are also likely to induce strong plasma bias and consequent
   poloidal and toroidal plasma rotation. Although backward electric
   currents, created in the plasma by intense LH pulses, dissipate a large
   portion of the radio frequency power absorbed: the current drive
   efficiency is acceptable. We use a numerical simulation of
   wave-particle interactions to analyse the applicability of standard
   quasilinear theory to the case of large energy flux densities. The
   initial results indicate the existence of important restrictions on the
   use of the quasilinear approximation. The results of the present paper
   also indicate that some of the effects considerably alter some ideas of
   Cohen et al.}},
ISSN = {{0022-3778}},
Unique-ID = {{ISI:000083492100006}},
}

@article{ ISI:000079691900002,
Author = {Krlin, L and Stockel, J and Svoboda, V},
Title = {{Stochastic (E x B) diffusion of ions in a spatially periodical
   potential field}},
Journal = {{PLASMA PHYSICS AND CONTROLLED FUSION}},
Year = {{1999}},
Volume = {{41}},
Number = {{3}},
Pages = {{339-353}},
Month = {{MAR}},
Abstract = {{This paper discusses the stochastic (E x B) motion of ions in the
   homogeneous magnetic field and in the spatially periodical
   electrostatic potential. This approach is used for the simulation of
   ion diffusion in potential structures formed at the boundary plasma in
   tokamaks. We show that the ion motion in such structures scales with a
   single dimensionless parameter R, which is a combination of amplitude
   and dimension of potential structures, magnetic field intensity and ion
   charge/mass ratio. The presented results show that for sufficiently
   large values of R, the intrinsic stochasticity regime of ion motion
   appears, which results in the spatial diffusion of ions. The diffusion
   coefficient is sufficiently high for heavy impurities. Possible further
   applications are mentioned.}},
ISSN = {{0741-3335}},
Unique-ID = {{ISI:000079691900002}},
}

